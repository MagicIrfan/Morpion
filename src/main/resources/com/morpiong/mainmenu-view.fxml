<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainMenuPane" prefHeight="600.0" prefWidth="600.0" stylesheets="@styles/button.css, @styles/mainMenu.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.morpiong.controller.MainMenuController">
   <AnchorPane fx:id="optionsPane" layoutX="201.0" prefHeight="600.0" prefWidth="200.0">
      <children>
         <Button fx:id="playButton" layoutY="285.0" mnemonicParsing="false" onAction="#onPlayButton" prefHeight="31.0" prefWidth="201.0" text="Jouer en 1 contre 1">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Button>
         <Button fx:id="quitButton" layoutY="423.0" mnemonicParsing="false" onAction="#onQuitButton" prefHeight="31.0" prefWidth="201.0" text="Quitter">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Button>
         <Label fx:id="titleLabel" layoutY="89.0" prefHeight="51.0" prefWidth="201.0" text="MORPION">
         </Label>
         <Button fx:id="playBotButton" layoutY="352.0" mnemonicParsing="false" onAction="#onPlayBotButton" prefHeight="31.0" prefWidth="201.0" text="Jouer contre un bot">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Button>
      </children>
   </AnchorPane>
</AnchorPane>
