<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainMenuPane" prefHeight="600.0" prefWidth="600.0" stylesheets="@styles/button.css, @styles/mainMenu.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.morpiong.controller.MainMenuController">
   <AnchorPane fx:id="optionsPane" layoutX="201.0" prefHeight="600.0" prefWidth="200.0">
      <children>
         <Button fx:id="playButton" layoutY="249.0" mnemonicParsing="false" onAction="#onPlayButton" prefHeight="31.0" prefWidth="201.0" stylesheets="@styles/button.css" text="Jouer en 1 contre 1">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor>
            <font>
               <Font name="Arial" size="12.0" />
            </font></Button>
         <Button fx:id="quitButton" layoutY="456.0" mnemonicParsing="false" onAction="#onQuitButton" prefHeight="31.0" prefWidth="201.0" text="Quitter">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Button>
         <Label fx:id="titleLabel" layoutY="89.0" prefHeight="51.0" prefWidth="201.0" text="MORPION" textFill="WHITE">
            <font>
               <Font name="Stencil" size="45.0" />
            </font></Label>
         <Button fx:id="playBotButton" layoutY="318.0" mnemonicParsing="false" onAction="#onPlayerVsBotButton" prefHeight="31.0" prefWidth="201.0" text="Jouer contre un bot">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Button>
         <Button fx:id="playBotVsBotButton" layoutY="387.0" mnemonicParsing="false" onAction="#onPlayBotVsBotButton" prefHeight="31.0" prefWidth="201.0" text="Un bot contre un bot">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor>
         </Button>
      </children>
   </AnchorPane>
</AnchorPane>
